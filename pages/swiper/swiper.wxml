<!--pages/swiper/swiper.wxml-->
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" duration="{{duration}}" circular="{{circular}}" indicator-color="#fff" indicator-active-color="#af0">
  <block wx:for="{{imageUrl}}" wx:key="{{index}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image"></image>
    </swiper-item>
  </block>
</swiper>